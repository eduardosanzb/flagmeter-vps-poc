{
  "title": "FlagMeter - Database & Services Monitoring",
  "tags": [
    "flagmeter",
    "databases",
    "services"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "PostgreSQL - Active Connections",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "pg_stat_activity_count{datname=\"flagmeter\"}",
          "legendFormat": "Active Connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 2,
      "title": "PostgreSQL - Database Size",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "pg_database_size_bytes{datname=\"flagmeter\"}",
          "legendFormat": "Database Size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "bytes"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 3,
      "title": "PostgreSQL - Cache Hit Ratio",
      "type": "gauge",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "100 * (sum(rate(pg_stat_database_blks_hit{datname=\"flagmeter\"}[5m])) / (sum(rate(pg_stat_database_blks_hit{datname=\"flagmeter\"}[5m])) + sum(rate(pg_stat_database_blks_read{datname=\"flagmeter\"}[5m]))))",
          "legendFormat": "Cache Hit %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 80,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 4,
      "title": "PostgreSQL - Transaction Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit{datname=\"flagmeter\"}[5m])",
          "legendFormat": "Commits/sec",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_xact_rollback{datname=\"flagmeter\"}[5m])",
          "legendFormat": "Rollbacks/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "ops"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
