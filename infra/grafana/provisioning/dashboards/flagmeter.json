{
  "dashboard": {
    "title": "FlagMeter Monitoring",
    "tags": ["flagmeter", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Latency (p50/p95/p99)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ]
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "redis_queue_length{queue=\"events\"}",
            "legendFormat": "Events Queue",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Queue Length"
          }
        ]
      },
      {
        "id": 3,
        "title": "PostgreSQL Active Connections",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          }
        ]
      },
      {
        "id": 4,
        "title": "Worker Concurrency",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "worker_concurrent_jobs",
            "legendFormat": "Active Jobs",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Jobs"
          }
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"api\"}",
            "legendFormat": "API",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"worker\"}",
            "legendFormat": "Worker",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          }
        ]
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"api\"}[5m])",
            "legendFormat": "API",
            "refId": "A"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"worker\"}[5m])",
            "legendFormat": "Worker",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "CPU"
          }
        ]
      },
      {
        "id": 7,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"api\"}[5m])",
            "legendFormat": "{{method}} {{path}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 8,
        "title": "Worker Job Processing Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(worker_jobs_processed_total[5m])",
            "legendFormat": "Jobs/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Jobs/sec"
          }
        ]
      }
    ]
  }
}
