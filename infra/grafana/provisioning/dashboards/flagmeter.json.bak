{
  "title": "FlagMeter - API, Database & Services Monitoring",
  "tags": [
    "flagmeter",
    "api",
    "databases",
    "services"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 100,
      "title": "HTTP - Request Rate (RPS)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\"}[1m]))",
          "legendFormat": "Total RPS",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\"}[1m])) by (http_route)",
          "legendFormat": "{{http_route}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 101,
      "title": "HTTP - Success Rate",
      "type": "gauge",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "100 * (sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=~\"2..\"}[5m])) / sum(rate(http_server_requests_total{service_name=\"dashboard\"}[5m])))",
          "legendFormat": "Success Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 95,
                "color": "yellow"
              },
              {
                "value": 99,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 102,
      "title": "HTTP - P95 Latency",
      "type": "gauge",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_bucket{service_name=\"dashboard\"}[5m])) by (le))",
          "legendFormat": "P95 Latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 200,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 103,
      "title": "HTTP - Requests by Status Code",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"200\"}[1m]))",
          "legendFormat": "200 OK",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"201\"}[1m]))",
          "legendFormat": "201 Created",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"400\"}[1m]))",
          "legendFormat": "400 Bad Request",
          "refId": "C"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"404\"}[1m]))",
          "legendFormat": "404 Not Found",
          "refId": "D"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"500\"}[1m]))",
          "legendFormat": "500 Internal Error",
          "refId": "E"
        },
        {
          "expr": "sum(rate(http_server_requests_total{service_name=\"dashboard\",http_status_code=\"503\"}[1m]))",
          "legendFormat": "503 Service Unavailable",
          "refId": "F"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
