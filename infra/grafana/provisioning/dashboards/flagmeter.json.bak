    {
      "id": 105,
      "title": "HTTP - Average Latency by Route",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "rate(http_server_duration_sum{service_name=\"dashboard\"}[5m]) / rate(http_server_duration_count{service_name=\"dashboard\"}[5m])",
          "legendFormat": "{{http_route}} - {{http_method}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "ms"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "lastNotNull",
            "mean"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 106,
      "title": "HTTP - Request Count by Endpoint",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(http_server_requests_total{service_name=\"dashboard\"}) by (http_route, http_method)",
          "legendFormat": "{{http_method}} {{http_route}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              },
              {
                "value": 100,
                "color": "green"
              },
              {
                "value": 1000,
                "color": "yellow"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name",
        "orientation": "horizontal"
      }
    },
